{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "camelcase": "warn",
        "consistent-return": "off",
        "max-len": "off",
        "no-console": "error",
        "no-param-reassign": "warn",
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "Promise|err"
            }
        ],
        "object-curly-newline": [
            "error",
            {
                "consistent": true
            }
        ]
    },
    "parser": "babel-eslint"
}